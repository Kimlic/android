<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/background">
    <shape>
      <corners android:radius="5dip"/>
      <gradient
        android:angle="270"
        android:centerColor="#FFE0E9F0"
        android:centerY="0.75"
        android:endColor="#FFE0E9F0"
        android:startColor="#FFE0E9F0"
        />
    </shape>
  </item>

  <item android:id="@android:id/secondaryProgress">
    <clip>
      <shape>
        <corners android:radius="5dip"/>
        <gradient
          android:angle="270"
          android:centerColor="#FFBFCFDB"
          android:centerY="0.75"
          android:endColor="#FFBFCFDB"
          android:startColor="#FFBFCFDB"
          />
      </shape>
    </clip>
  </item>

  <item android:id="@android:id/progress">
    <clip>
      <shape>
        <corners android:radius="5dip"/>
        <gradient
          android:angle="270"
          android:centerColor="#FFBFCFDB"
          android:centerY="0.75"
          android:endColor="#FFBFCFDB"
          android:startColor="#FFBFCFDB"
          />
      </shape>
    </clip>
  </item>

</layer-list>

